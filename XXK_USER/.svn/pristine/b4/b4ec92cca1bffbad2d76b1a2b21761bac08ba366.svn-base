/* 按钮 */
.btn_all {
	height:21px;
	line-height:21px;
	text-align:center;
	font-size:12px;
	color:#000;
	background:url(../images/button/btn_all.gif) left top repeat-x;
	border:1px solid #8fb9d0;
	padding:0px 1px;;
	vertical-align: top;
	text-align:center;
	cursor: pointer;
	}
/* 列表 */
#man_list {
	width: 100%;
	margin: 10px 0px 5px 10px;
	padding: 0px;
	overflow: auto;
}
.tit_list{ height:24px;background-color: #d2e0f3; text-align:center; font-weight:bold; font-size:14px; letter-spacing:10px;}
.tit_list2{ height:20px;background-color: #F3F8F7; text-align:center; font-weight:bold; font-size:12px; color:#444444; letter-spacing:2px;}
.tit_list3{ height:15px;background-color: #F3F8F7; text-align:center; font-weight:bold; font-size:12px; color:#444444;}
.tit_list4{ height:15px;text-align:center; font-weight:bold; font-size:20px; color:#444444;}
.tit_list5{ height:20px;background-color: #F3F8F7; text-align:center; font-size:12px; color:#444444; }
.tit_list6{ height:20px;text-align:left;text-indent:2em; font-size:15px; color:#444444; }

.td_list {color: #344b50; font-size: 12px;font-weight:bold;  text-align:center;height:27px; }
.td_list_info {color: #000000;font-size: 12px;text-align:center;height:25px;padding-left: 5px;}
.link {
	font-size: 12px;
	color: #295568;
}

/* 切换式选项卡 */
.menu div{ float:left; width:100px; height:20px; margin-right:2px;}
.menu ul{list-style:none;}
.menu li{list-style:none; cursor:pointer; display:inline;}
.info{ border:1px solid #a8c7ce; width:99%; min-height:200px;clear:both; margin-top:-1px;padding-top:5px; padding-left:5px;padding-bottom:5px;}
.over_li div, .out_li div { letter-spacing:1px;; border:1px #a8c7ce solid; background:#a8c7ce;float:left; text-align:center;padding-top:10px; }
.over_li div {background-color:#83a6ae; border-bottom:none;}
/* end-切换式选项卡  */

