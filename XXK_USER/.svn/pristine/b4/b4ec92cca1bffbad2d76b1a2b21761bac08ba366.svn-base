@charset "utf-8";

body {
font-size:12px;
font-family:"宋体",Arial, Helvetica, sans-serif;
}
/*去除A标签点击之后的虚线*/
a {outline: none;}
a:active {star:expression(this.onFocus=this.blur());}/*只IE识别*/
:focus { outline:0; }
/*去除A标签点击之后的虚线*/
*{ margin:0px;}
a {
	color: #344b50;
	text-decoration:none;
}
a:hover {
	color: #0099CC; 
}
.txt{ height:15px;}
.hei_per{height:1px;line-height: 2px;}/* 分隔5像素 */
.hei_per2{height:2px;line-height: 2px;}/* 分隔5像素 */
.table{
	 font-size:12px;
	}

.tit{background-color: #bfd4d8; text-align:center; font-weight:bold; font-size:14px; color:#444444; letter-spacing:15px;}

.man_zone {
	min-height: 100px;
	width: 100%;
	border: 1px solid #B4C9C6;
	margin: 5px 0 0 5px;
	background-color:#FFFFFF;
	padding: 5px 0 5px 0;
	overflow: auto;
}
.man_zone table{
    background-color:#a8c7ce;
}
.man_zone table tr{
    background-color:#fff;
}

.man_zone2{
	font-size: 12px;
}
.man_zone2 table,.man_zone2 table tr td {
	border:1px solid #a8c7ce;
	border-collapse:collapse;
}
.man_zone2 table tr td{padding:3px 5px;}

