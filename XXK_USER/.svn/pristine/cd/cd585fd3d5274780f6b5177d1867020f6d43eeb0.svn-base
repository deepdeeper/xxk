package com.lzzyyy.xxk.user.pojo;

import java.util.HashSet;
import java.util.Set;

public class SysMenu {
	private String menuId;

	private String menuName;

	private Integer menuLevel;

	private Integer menuDisplayLevel;

	private String menuPic;

	private String menuUrl;

	private String remark;
	
	private Set<SysRole> roles = new HashSet<SysRole>(0);//一个菜单对应多个角色
	
	private SysMenu parentMenu;//父菜单
	
	private Set<SysMenu>  chlidMenus = new HashSet<SysMenu>(0);//子菜单

	public String getMenuId() {
		return menuId;
	}

	public void setMenuId(String menuId) {
		this.menuId = menuId;
	}

	public String getMenuName() {
		return menuName;
	}

	public void setMenuName(String menuName) {
		this.menuName = menuName;
	}

	public Integer getMenuLevel() {
		return menuLevel;
	}

	public void setMenuLevel(Integer menuLevel) {
		this.menuLevel = menuLevel;
	}

	public Integer getMenuDisplayLevel() {
		return menuDisplayLevel;
	}

	public void setMenuDisplayLevel(Integer menuDisplayLevel) {
		this.menuDisplayLevel = menuDisplayLevel;
	}

	public String getMenuPic() {
		return menuPic;
	}

	public void setMenuPic(String menuPic) {
		this.menuPic = menuPic;
	}

	public String getMenuUrl() {
		return menuUrl;
	}

	public void setMenuUrl(String menuUrl) {
		this.menuUrl = menuUrl;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public Set<SysRole> getRoles() {
		return roles;
	}

	public void setRoles(Set<SysRole> roles) {
		this.roles = roles;
	}

	public SysMenu getParentMenu() {
		return parentMenu;
	}

	public void setParentMenu(SysMenu parentMenu) {
		this.parentMenu = parentMenu;
	}

	public Set<SysMenu> getChlidMenus() {
		return chlidMenus;
	}

	public void setChlidMenus(Set<SysMenu> chlidMenus) {
		this.chlidMenus = chlidMenus;
	}

	
	
}
