<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="user" extends="struts-default">

		<action name="userInfo_*"
			class="com.lzzyyy.xxk.user.web.struts2.action.UserAction" method="{1}">
	
			<result name="loginSuccess">/jsp/user/loginOK.jsp</result>
			<result name="listUser">/jsp/user/listUser.jsp</result>
			<result name="login">/login/login.jsp</result>
			<result name="index">/general/index.jsp</result>
			<result name="toAddUser">/general/user/userAdd.jsp</result>




		</action>
	</package>

</struts>