<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.lzzyyy.xxk.user.pojo.SysRole" table="Sys_Role">
        <id name="roleId" type="string" column="roleId" length="32">
            <generator class="uuid" />
        </id>
        <property name="roleName" type="string">
            <column name="roleName" length="30" not-null="true" />
        </property>
        <property name="securityLevel" type="integer"></property>
      
        <property name="remark" type="string">
            <column name="remark" length="200" />
        </property>
        
        <set name="users" inverse="true" lazy="extra" cascade="all">
			<key>
				<column name="roleId" length="32" />
			</key>
			<one-to-many class="com.lzzyyy.xxk.user.pojo.User" />
		</set>
        
      
    </class>
</hibernate-mapping>
